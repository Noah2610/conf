
clear Lock
!keycode 66 = Escape

keycode 91 = period period

! scroll:
keycode 44 = j J NoSymbol NoSymbol Next
keycode 45 = k K NoSymbol NoSymbol Prior

! space to super/i3-mod:
!keycode 65 = Super_L
!keycode any = space
keycode 65 = space space

! capslock/escape to super/i3-mod:
keycode 66 = Super_R
keycode any = Escape


!keycode 66 = Caps_Lock
!add Mod4 = Super_R
! set right shift to super
!clear Shift
!add Mod4 = Shift_R
!add Shift = Shift_L

